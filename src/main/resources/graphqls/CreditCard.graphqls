type CreditCard implements Entity {
    id: ID!
    number: String!
    goodThru: String!
    brand: Brand!
    securityCode: String!
}

type Query {
    findCreditCards: [CreditCard]!
}

type Mutation {
    createCreditCard(form: CreditCardForm!): CreditCard!
}

input CreditCardPayLoad {
    number: String!
    goodThru: String!
    brandId: Int!
    securityCode: String!
}
